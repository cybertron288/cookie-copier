{
  "name": "Cookie Copier",
  "version": "1.1",
  "manifest_version": 3,
  "permissions": ["cookies", "activeTab", "storage"],
  "host_permissions": ["*://*/*"],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/logo.png",
    "48": "icons/logo.png",
    "128": "icons/logo.png"
  }
}

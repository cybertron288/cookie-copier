{
  "name": "cookie-copier",
  "displayName": "Cookie Copier",
  "version": "2.0.0",
  "description": "A professional Chrome extension for copying and managing cookies across domains with enhanced security and user experience",
  "main": "dist/popup.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.{js,ts,css,html}",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "zip": "pnpm run build && zip -r cookie-copier-extension.zip dist/"
  },
  "keywords": [
    "chrome-extension",
    "cookies",
    "developer-tools",
    "session-management",
    "testing",
    "authentication"
  ],
  "author": {
    "name": "cybertron288",
    "email": "ravi60353@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cybertron288/cookie-copier.git"
  },
  "bugs": {
    "url": "https://github.com/cybertron288/cookie-copier/issues"
  },
  "homepage": "https://github.com/cybertron288/cookie-copier#readme",
  "devDependencies": {
    "@crxjs/vite-plugin": "2.2.0",
    "@tailwindcss/forms": "0.5.10",
